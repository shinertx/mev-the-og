# Example .env file for MEV The OG
# Copy this file to .env and fill in actual secrets. DO NOT COMMIT .env TO GIT!

# Mainnet & Sepolia Alchemy RPC URLs
ALCHEMY_API_KEY_MAINNET_RPC_URL=https://eth-mainnet.alchemyapi.io/v2/your-api-key
ALCHEMY_API_KEY_SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your-api-key
INFURA_API_KEY_MAINNET_RPC_URL=https://mainnet.infura.io/v3/your-infura-key

# Telegram notifier
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-telegram-chat-id

# Cloud KMS/Signer credentials
KMS_API_KEY=your-kms-api-key
KMS_KEY_ID=your-kms-key-id

# Local signer (if applicable)
LOCAL_SIGNER_KEY_PATH=/secure/path/to/keystore

# Any other sensitive env variables go here
